{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [{
			"name": "client",
			"image": "george/multi-client",
			"hostname": "client",
			"essential": false
		},
		{
			"name": "nginx",
			"image": "george/multi-nginx",
			"hostname": "nginx",
			"essential": true,
			"portMappings": [
				{"containerPort": 80,"hostPort": 80}
							],
			"links": ["client", "server"]
		},
		{
			"name": "server",
			"image": "george/multi-server",
			"hostname": "api",
			"essential": false
		},
		{
			"name": "worker",
			"image": "george/multi-worker",
			"hostname": "worker",
			"essential": false
		}
	]
}